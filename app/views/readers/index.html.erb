<table>
    <tr>
        <th>ID</th>
        <th>Active</th>
        <th>Fist Name</th>
        <th>Last Name</th>
        <th>DOB</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Street Address 1</th>
        <th>Street Address 2</th>
        <th>City</th>
        <th>Zip</th>
    </tr>
    <% @readers.each do |r| %>
        <tr>
            <td><%= r.id %></td>
            <td><%= r.active %></td>
            <td><%= r.first_name %></td>
            <td><%= r.last_name %></td>
            <td><%= r.dob %></td>
            <td><%= r.phone %></td>
            <td><%= r.email %></td>
            <td><%= r.address_line1 %></td>
            <td><%= r.address_line2 %></td>
            <td><%= r.city %></td>
            <td><%= r.zip %></td>
        </tr>
    <%end %>
    
</table>